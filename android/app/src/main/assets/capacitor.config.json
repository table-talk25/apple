{
	"appId": "com.tabletalk.socialapp",
	"appName": "TableTalk",
	"webDir": "FRONTEND/client/build",
	"server": {
		"androidScheme": "https"
	},
	"plugins": {
		"GoogleAuth": {
			"scopes": [
				"profile",
				"email"
			],
			"serverClientId": "925236799140-op5ejjto2gitab71egav5voossk36h1b.apps.googleusercontent.com"
		},
		"SplashScreen": {
			"launchShowDuration": 2000,
			"backgroundColor": "#667eea",
			"showSpinner": true,
			"spinnerColor": "#ffffff"
		},
		"PushNotifications": {
			"presentationOptions": [
				"badge",
				"sound",
				"alert"
			]
		}
	}
}
